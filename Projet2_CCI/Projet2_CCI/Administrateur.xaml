<Window x:Class="Projet2_CCI.Administrateur"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projet2_CCI"
        mc:Ignorable="d"
        Title="Administrateur" Height="450" Width="800">
    <Grid>
        <ListBox HorizontalAlignment="Left" Height="399" Margin="10,10,0,0" VerticalAlignment="Top" Width="100" x:Name="listeUtilisateurs" ItemsSource="{Binding stringListe}" SelectionChanged="ListeUtilisateurs_SelectionChanged">
        </ListBox>
        <Button Content="Button" HorizontalAlignment="Left" Margin="393,389,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="412,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" x:Name="test" />
    </Grid>
</Window>
